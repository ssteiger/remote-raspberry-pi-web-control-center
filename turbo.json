{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
        "outputs": [".next/**", "!.next/cache/**"]
    },
    "dev:web-app": {
      "dependsOn": ["dev:backend"],
      "cache": false
    },
    "dev:backend": {
      "cache": false
    },
    "dev": {
      "dependsOn": ["env:check", "dev:web-app", "dev:backend"],
      "cache": false
    },
    "env:check": {
      "cache": false,
      "persistent": false
    }
  }
}
